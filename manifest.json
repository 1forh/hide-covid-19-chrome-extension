{
  "name": "Hide Covid-19",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Hides text blocks mentioning Covid-19.",
  "homepage_url": "http://zacharyminner.com",
  "background": {
    "scripts": ["src/background.js"]
  },
  "browser_action": {
    "default_icon": {
      "48": "src/media/icon-48.png"
    }
  },
  "permissions": ["activeTab", "tabs", "background", "http://*/*", "https://*/*"]
}
